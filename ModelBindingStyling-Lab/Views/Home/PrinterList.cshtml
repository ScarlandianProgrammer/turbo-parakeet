@model IEnumerable<ThreeDPrinters>

@{
    ViewData["Title"] = "Printer List";
}

<h1>PrinterList</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SKU)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BuildVolume)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrinterImage)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
    <div class="row row-cols-1 row-cols-md-3 g-4">
        <div class="col">
            <div class="card">
                <img src="@Html.DisplayFor(modelItem => item.PrinterImage)" class="card-img-top" alt="...">
                <div class="card-body">
                    <h5 class="card-title">@Html.DisplayFor(modelItem => item.Title)</h5>
                    <p class="card-text">@Html.DisplayFor(modelItem => item.SKU)</p>
                    <p class="card-text">@Html.DisplayFor(modelItem => item.Price)</p>
                    <p class="card-text">@Html.DisplayFor(modelItem => item.BuildVolume)</p>
                </div>
            </div>
        </div>
    </div>
        
}
    </tbody>
</table>
